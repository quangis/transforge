@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.

:Transformation
    a rdfs:Class,
    rdfs:comment "The class of conceptual transformations expressible in a transformation algebra.".

:operation
    a rdf:Property;
    rdfs:label "contains operation";
    rdfs:comment "A constituent operation of a transformation.";
    rdfs:domain :Transformation;
    rdfs:range :Operation.

:data
    a rdf:Property;
    rdfs:label "contains data";
    rdfs:comment "Some input, intermediary or output data of a transformation.";
    rdfs:domain :Transformation;
    rdfs:range :Data.

:target
    a rdf:Property;
    rdfs:comment "The output data for a transformation as a whole.";
    rdfs:domain :Transformation;
    rdfs:range :Data.

:Operation
    a rdfs:Class;
    rdfs:comment "The class of operations; representing the atomary constituents of conceptual transformations.".

:InternalOperation
    a rdfs:Class;
    rdfs:subClassOf :Operation;
    rdfs:comment "Any internal process for which the structure is unknown and that produces internal data.".

:Data
    a rdfs:Class;
    rdfs:comment "The class of data (input, output or intermediary) relevant to conceptual transformations.".

:InternalData
    a rdfs:Class;
    rdfs:subClassOf :Data;
    rdfs:comment "Data produced by any operation that is passed as parameter to another operation. The structure of this internal data is unknown.".

:source
    a rdf:Property;
    rdfs:comment "The source for some input data.";
    rdfs:domain :Data.

:input
    a rdf:Property;
    rdfs:comment "The input data for an operation.";
    rdfs:domain :Operation;
    rdfs:range :Data.

:output
    a rdf:Property;
    rdfs:comment "The output data for an operation.";
    rdfs:domain :Operation;
    rdfs:range :Data.

:internal
    a rdf:Property;
    rdfs:comment "An operation that is internal to another operation.";
    rdfs:domain :Operation;
    rdfs:range :Operation.

# Types.
:type
    a rdf:Property;
    rdfs:label "has type";
    rdfs:comment "The type of some data or operation.";
    rdfs:range :Type.
:Type
    a rdfs:Class;
    rdfs:comment "The class of all types".
:TypeVar
    rdfs:comment "The class of type variables.";
    rdfs:subClassOf :Type.
:TypeOperator
    rdfs:comment "The class of concrete types.";
    rdfs:subClassOf :Type.
:Function
    rdfs:comment "The class of type operations for functions.";
    a :TypeOperator.
